    "rollup-plugin-node-externals": "^2.1.3",
    "sinon": "^8.1.0"
  },
  "dependencies": {
    "ajv": "^6.11.0",
    "chalk": "^2.4.2",
    "ejs": "^2.6.1",
    "glob": "^7.1.6",
    "hosted-git-info": "^6.1.1",
    "lodash": "^4.17.15",
    "minimatch": "^3.0.4",
    "mustache": "^4.0.0",
    "prompts": "^2.3.0",
    "semver": "^7.1.3",
    "string": "^3.3.3",
    "tar": "^5.0.5",
    "update-notifier": "^4.0.0",
    "winston": "^3.2.1",
    "yargs": "^13.3.0"
